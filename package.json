    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "gif-encoder": "^0.7.2",
    "lzma-native": "^8.0.6",
    "pngjs": "^7.0.0",
    "tetris-fumen": "^1.1.3",
    "wx-svelte-grid": "^2.1.5"
