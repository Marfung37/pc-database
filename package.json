    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.10.1",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "mdsvex": "^0.12.6",
    "prettier": "^3.6.2",
    "prettier-plugin-sql": "^0.19.1",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "svelte": "^5.35.1",
    "svelte-check": "^4.2.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "yargs": "^17.7.2"
