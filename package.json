{
  "name": "pc-database",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "format_check": "prettier --check .",
    "lint": "eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test",
    "upload_paths": "DOTENV_CONFIG_PATH=./.env.local tsx -r dotenv/config utils/populatePathFiles.ts",
    "populate_saves": "DOTENV_CONFIG_PATH=./.env.local tsx -r dotenv/config utils/populateSaves.ts",
    "add_setups": "DOTENV_CONFIG_PATH=./.env.local tsx -r dotenv/config utils/addSetups.ts"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.54.2",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/adapter-vercel": "^5.10.1",
    "@sveltejs/kit": "^2.33.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.12",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/svelte": "^5.2.8",
    "@types/gif-encoder": "^0.7.4",
    "@types/lzma-native": "^4.0.4",
    "@types/pngjs": "^6.0.5",
    "@types/yargs": "^17.0.33",
    "csv-writer": "^1.6.0",
    "daisyui": "^5.0.50",
    "dotenv": "^16.6.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "mdsvex": "^0.12.6",
    "prettier": "^3.6.2",
    "prettier-plugin-sql": "^0.19.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.38.2",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@inlang/paraglide-js": "^2.2.0",
    "@lucide/svelte": "^0.511.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.6.0",
    "gif-encoder": "^0.7.2",
    "lzma-native": "^8.0.6",
    "pngjs": "^7.0.0",
    "svelte-sonner": "^1.0.5",
    "tetris-fumen": "^1.1.3",
    "wx-svelte-grid": "^2.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "lzma-native"
    ]
  }
}
